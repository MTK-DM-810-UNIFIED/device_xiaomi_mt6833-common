# Fingerprint
genfscon sysfs /devices/platform/11019000.spi5/spi_master/spi5/spi5.0/clk_enable                   u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11019000.spi5/spi_master/spi5/spi5.0/irq                          u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11019000.spi5/spi_master/spi5/spi5.0/wakeup_enable                u:object_r:vendor_sysfs_fingerprint:s0

# Health
genfscon sysfs /devices/platform/mt_charger/power_supply/ac                                        u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/battery/power_supply/battery                                      u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/mt_charger/power_supply/usb                                       u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/11e00000.i2c0/i2c-0/0-0066/power_supply/sc8551-standalone         u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/maxim_ds28e16/power_supply/batt_verify                            u:object_r:sysfs_batteryinfo:s0

# Wakeup (https://lkml.org/lkml/2019/8/6/1275)
genfscon sysfs /devices/platform/11019000.spi5/spi_master/spi5/spi5.0/wakeup                       u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11cb0000.i2c3/i2c-3/3-0008/wakeup                                 u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11d01000.i2c9/i2c-9/9-005a/wakeup                                 u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11e00000.i2c0/i2c-0/0-0066/power_supply/sc8551-standalone/wakeup  u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11e00000.i2c0/i2c-0/0-0066/wakeup                                 u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11e01000.i2c5/i2c-5/5-004e/tcpc/type_c_port0/wakeup               u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11e01000.i2c5/i2c-5/5-004e/wakeup                                 u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/maxim_ds28e16/power_supply/batt_verify/wakeup                     u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/virtual/input/input3/wakeup                                                u:object_r:sysfs_wakeup:s0
